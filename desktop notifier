# desktop notifier 

from plyer import notification
import time 

if __name__ == '__main__ ':
        while True:
            notification.notify(
                title="***take rest***",
                message=" its 11 pm take rest ",
                app_png="C:/Users/Asus/Documents/pythn/Project UTS Appunnamed.png",
                timeout=5)
            time.sleep(60*60)

